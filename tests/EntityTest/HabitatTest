<?php

namespace App\Tests\Entity;

use App\Entity\Habitat;
use PHPUnit\Framework\TestCase;

class HabitatTest extends TestCase
{
    public function testGettersAndSetters()
    {
        $habitat = new Habitat();
        $habitat->setNom('Test nom');
        $habitat->setDescription('Test Descritpion');
        $habitat->setCommentaire('Test Commentaire');

        $this->assertEquals('Test Prenom', $habitat->getNom());
        $this->assertEquals('Test Nombre de cliques', $habitat->getDescription());
        $this->assertEquals('Test Nombre de cliques', $habitat->getCommentaire());
     
    }
}